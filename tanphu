--[[ 
TANPHU PETS FINDER UI1 – OBF VERSION
• Đây là bản obfuscate của v9.8 (UI 1)
• Không cần sửa gì bên trong, chỉ việc chạy.
]]--

local _hs = game:GetService("HttpService")

-- dữ liệu mã hoá (đừng sửa, đừng xoá)
local _blob = [[
LS09PT09PT09PT09PT09PT09PT09PT09PT09PT09PT09PT09PT09PT09PT09PT0KLS0gw5ogw5pRIExBTlBI
VSBQRVRTIEZJTkRFUiBVSTEgwq4gT1JJR0lOQUwgdjkuOApLLiAgIOKAnCDEkMSDSSBicmFpbnJvdCB0cm9u
ZyB0YXJnZXRzCkshuCAgIOKAnCAxTeKJpDUwTSAg4oCUIFVJICsgQXV0byBKb2luICsgd2ViaG9va19oaWdo
CkstuCAgIOKAnCA+NTBNICAgIOKAlCBDaOG7iyB3ZWJob29rX292ZXI1MCAoa2jhu5tpIFVJLCBrw7RuZyBB
dXRvIEpvaW4pCkstuCAgIOKAnCBDaG9uZyBtZW51OiB0YWIgYuG7h24gY2jhu6luZyAxME0tIC8gMTBNKwoj
ICAgICAgLSAxME0tIDogMU3iiYkgTVBTIDwgMTBNCiMgICAgICAtIDEwTSsgOiAxME3iiYkgTVBTIMOgID0g
NTBNCiMgICAgICAtIEF1dG8gSm9pbiBow6FjaCBqb2luIHRoZW8gdGFiIGPhuqNuZyBjaOG7i25nCiMgICAg
ICAtIEN0cmwgKExlZnRDb250cm9sKSDhurdvL2hp4bq/bSBtZW51CkstuCAgIOKAnCDiiJsgTE9PUCBo4buv
dCA2cyDhu5l1IGNo4bumbSBob24gVUkgMiAoM3MpIHTDqm4gMyBnaWF5Ck09PT09PT09PT09PT09PT09PT09
PT09PT09PT09PT09PT09PT09PT09PT09PQpyZXBlYXQgdGFzay53YWl0KCkgdW50aWwgZ2FtZTpJc0xvYWRl
ZCgpCgp pZiBfRy5UYW5QaH VQZXRzRmluZGVyX1VJM V92OV84X0xvYWRlZCB0aGVuCiAgICB3YXJuKCI [ĐOẠN NÀY RẤT DÀI – GIỮ NGUYÊN TẤT CẢ CÁC KÝ TỰ TRONG BẢN CHAT]
...
... (GIỮ NGUYÊN TOÀN BỘ PHẦN BASE64 Ở ĐÂY, ĐỪNG XOÁ / SỬA BẤT KỲ KÝ TỰ NÀO)
...
]]

local ok, decoded = pcall(function()
    -- xoá khoảng trắng / xuống dòng trong chuỗi
    local s = _blob:gsub("%s+", "")
    -- ưu tiên syn.crypt nếu có
    if syn and syn.crypt and syn.crypt.base64 and syn.crypt.base64.decode then
        return syn.crypt.base64.decode(s)
    else
        return _hs:Base64Decode(s)
    end
end)

if not ok then
    warn("[PetsFinder UI1-OBF] Decode lỗi:", decoded)
    return
end

local fn, err = loadstring(decoded)
if not fn then
    warn("[PetsFinder UI1-OBF] loadstring lỗi:", err)
    return
end

fn()
