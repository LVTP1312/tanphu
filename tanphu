--========================
-- üì• ƒê·ªåC LIST PET T·ª™ DISCORD
--========================
local function fetchMessagesFromDiscord()
    if not http_request_impl or BOT_TOKEN == "" or CHANNEL_ID == "" then
        return {}
    end

    local url = ("https://discord.com/api/v10/channels/%s/messages?limit=40"):format(CHANNEL_ID)
    local res, err = safeRequest({
        Url = url,
        Method = "GET",
        Headers = {
            ["Authorization"] = "Bot " .. BOT_TOKEN
        }
    })
    if not res then
        warn("[GlobalPetsFinder] Discord HTTP error:", err)
        return {}
    end
    if res.StatusCode ~= 200 then
        warn("[GlobalPetsFinder] Discord Status:", res.StatusCode, res.Body or "")
        return {}
    end

    local ok, data = pcall(function()
        return HttpService:JSONDecode(res.Body)
    end)
    if not ok or type(data) ~= "table" then
        warn("[GlobalPetsFinder] JSON decode error")
        return {}
    end

    return data
end

-- Parse 1 message -> {name, mps, jobId}
local function parsePetFromMessage(msg)
    if not msg or type(msg.content) ~= "string" then return nil end
    local content = msg.content

    -- l·∫•y t√™n
    local name = content:match("üêæ%s*Name:%s*(.-)\n")
        or content:match("Name:%s*(.-)\n")
    -- l·∫•y MPS text
    local mpsText = content:match("üí∏%s*MPS:%s*([^\n]+)")
        or content:match("MPS:%s*([^\n]+)")
    -- l·∫•y JobId
    local jobId = content:match("üÜî%s*Job ID:%s*`([%w%-_]+)`")
        or content:match("Job ID:%s*`?([%w%-_]+)`?")

    if not (name and mpsText and jobId) then
        return nil
    end

    local mps = parseMps(mpsText)
    if mps < MIN_MPS or mps > SPLIT_MPS then
        return nil -- ch·ªâ l·∫•y 1M‚Äì50M cho UI n√†y
    end

    return {
        name  = name,
        mps   = mps,
        jobId = jobId,
    }
end

task.spawn(function()
    warn("[GlobalPetsFinder] Discord UI loop started.")
    while task.wait(FETCH_INTERVAL) do
        local msgs = fetchMessagesFromDiscord()

        clearScrolling(ScrollingLow)
        clearScrolling(ScrollingHigh)
        autoJoinedJobs = {}

        local seen = {} -- key = name|jobId

        -- Discord tr·∫£ newest tr∆∞·ªõc ‚Üí duy·ªát t·ª´ tr√™n xu·ªëng
        for _, msg in ipairs(msgs) do
            local pet = parsePetFromMessage(msg)
            if pet then
                local key = pet.name .. "|" .. pet.jobId
                if not seen[key] then
                    seen[key] = true

                    local bucket = (pet.mps >= TEN_MPS) and "HIGH" or "LOW"
                    if bucket == "LOW" then
                        AddPetItem(ScrollingLow, pet.name, pet.mps, pet.jobId)
                    else
                        AddPetItem(ScrollingHigh, pet.name, pet.mps, pet.jobId)
                    end

                    -- Auto join
                    if autoJoinEnabled and bucket == currentTab and not autoJoinedJobs[pet.jobId] then
                        autoJoinedJobs[pet.jobId] = true
                        pcall(function()
                            TeleportService:TeleportToPlaceInstance(game.PlaceId, pet.jobId, LocalPlayer)
                        end)
                    end
                end
            end
        end
    end
end)

--========================
-- üîç OPTIONAL: SCANNER LOCAL + WEBHOOK
--========================
if ENABLE_SCANNER then
    local bestMpsPerPet = {}

    local function getMpsFromModel(pet)
        local mpsObj = pet:FindFirstChild("MPS")
        if mpsObj and mpsObj.Value ~= nil then
            local raw = tostring(mpsObj.Value)
            return parseMps(raw), raw
        end

        local bestText = nil
        for _, d in ipairs(pet:GetDescendants()) do
            if d:IsA("TextLabel") or d:IsA("TextButton") then
                local t = d.Text
                if t and t ~= "" then
                    local up = t:upper()
                    if up:find("/S") or up:find("M/S") or up:find("PERSEC") then
                        bestText = t
                        break
                    end
                end
            end
        end

        if bestText then
            return parseMps(bestText), bestText
        end

        return 0, nil
    end

    task.spawn(function()
        warn("[GlobalPetsFinder] Local scanner started.")
        while task.wait(6) do
            local jobId   = tostring(game.JobId or "N/A")
            local placeId = game.PlaceId

            local candidates = {}
            local petsFolder = workspace:FindFirstChild("Pets")

            if petsFolder then
                candidates = petsFolder:GetChildren()
            else
                for _, inst in ipairs(workspace:GetChildren()) do
                    if inst:IsA("Model") or inst:IsA("Folder") then
                        table.insert(candidates, inst)
                    end
                end
            end

            for _, pet in ipairs(candidates) do
                local displayName = detectBrainrotName(pet)
                if not displayName then
                    continue
                end

                local mps = select(1, getMpsFromModel(pet))
                if mps < MIN_MPS then
                    continue
                end

                local prevBest = bestMpsPerPet[pet] or 0
                if mps <= prevBest then
                    continue
                end
                bestMpsPerPet[pet] = mps

                local over50 = (mps > SPLIT_MPS)
                local joinLink = ("https://chillihub1.github.io/chillihub-joiner/?placeId=%s&gameInstanceId=%s")
                    :format(placeId, jobId)

                local title = over50
                    and "üîî **Pet >50M Found! (Brainrot)**"
                    or  "üîî **Pet Found! (Brainrot, 1M‚Äì50M)**"

                local content = table.concat({
                    title,
                    "üêæ Name: " .. displayName,
                    "üí∏ MPS: " .. formatMpsShort(mps) .. "/s",
                    "üÜî Job ID: `" .. jobId .. "`",
                    "üåê [JOIN HERE](" .. joinLink .. ")"
                }, "\n")

                if over50 then
                    sendWebhook(WEBHOOK_OVER50, { content = content })
                else
                    sendWebhook(WEBHOOK_HIGH, { content = content })
                end
            end
        end
    end)
end
