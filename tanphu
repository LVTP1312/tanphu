local function _decodeChunk(s)
    s = tostring(s or ""):gsub("%s+", "")
 
    local alphabet = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/"

    -- lá»c bá» kÃ½ tá»± láº¡ (phÃ²ng trÆ°á»ng há»£p cÃ³ xuá»‘ng dÃ²ng, khoáº£ng tráº¯ngâ€¦)
    s = s:gsub("[^" .. alphabet .. "=]", "")

    -- má»—i kÃ½ tá»± => 6 bit
    local bits = s:gsub(".", function(ch)
        if ch == "=" then
            return ""
        end
        local v = (alphabet:find(ch) or 1) - 1
        local out = ""
        for i = 6, 1, -1 do
            local bit = v % 2^i - v % 2^(i-1) > 0 and "1" or "0"
            out = out .. bit
        end
        return out
    end)

    -- gom tá»«ng 8 bit => 1 char
    local out = bits:gsub("%d%d%d?%d?%d?%d?%d?%d?", function(chunk)
        if #chunk < 8 then return "" end
        local n = 0
        for i = 1, 8 do
            if chunk:sub(i,i) == "1" then
                n = n + 2^(8-i)
            end
        end
        return string.char(n)
    end)

    return out
end

--==================================================
-- ðŸ“¦ CHUá»–I DATA ÄÃƒ MÃƒ HOÃ
--   ðŸ‘‰ BÆ¯á»šC 1: copy NGUYÃŠN chuá»—i cÅ© cá»§a báº¡n (tá»« [[ ... ]])
--   ðŸ‘‰ BÆ¯á»šC 2: dÃ¡n vÃ o giá»¯a [[ ... ]] dÆ°á»›i Ä‘Ã¢y
--==================================================

local _blob = [[
PUT_YOUR_OLD_ENCODED_STRING_HERE
]]

--==================================================
-- ðŸš€ GIáº¢I MÃƒ + CHáº Y SCRIPT Gá»C
--==================================================

local ok, decoded = pcall(_decodeChunk, _blob)
if not ok then
    warn("[PetsFinder OBF] Decode lá»—i:", decoded)
    return
end

local fn, err = loadstring(decoded)
if not fn then
    warn("[PetsFinder OBF] loadstring lá»—i:", err)
    return
end

fn()
