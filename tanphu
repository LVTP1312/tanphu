--------------------------------------------------
-- üîì H√ÄM GI·∫¢I M√É (KH√îNG D√ôNG HttpService / Base64Decode)
--------------------------------------------------
local function _decodeChunk(s)
    s = tostring(s or ""):gsub("%s+", "")

    -- b·ªô k√Ω t·ª± d√πng ƒë·ªÉ m√£ ho√°
    local alphabet = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/"

    -- l·ªçc b·ªè k√Ω t·ª± l·∫° (xu·ªëng d√≤ng, kho·∫£ng tr·∫Øng‚Ä¶)
    s = s:gsub("[^" .. alphabet .. "=]", "")

    -- m·ªói k√Ω t·ª± => 6 bit
    local bits = s:gsub(".", function(ch)
        if ch == "=" then
            return ""
        end
        local v = (alphabet:find(ch) or 1) - 1
        local out = ""
        for i = 6, 1, -1 do
            local bit = v % 2^i - v % 2^(i-1) > 0 and "1" or "0"
            out = out .. bit
        end
        return out
    end)

    -- gom t·ª´ng 8 bit => 1 k√Ω t·ª± ASCII
    local out = bits:gsub("%d%d%d?%d?%d?%d?%d?%d?", function(chunk)
        if #chunk < 8 then return "" end
        local n = 0
        for i = 1, 8 do
            if chunk:sub(i,i) == "1" then
                n = n + 2^(8-i)
            end
        end
        return string.char(n)
    end)

    return out
end

--------------------------------------------------
-- üöÄ GI·∫¢I M√É + CH·∫†Y SCRIPT G·ªêC
--------------------------------------------------
local ok, decoded = pcall(_decodeChunk, _blob)
if not ok then
    warn("[PetsFinder UI1-OBF] Decode l·ªói:", decoded)
    return
end

local fn, err = loadstring(decoded)
if not fn then
    warn("[PetsFinder UI1-OBF] loadstring l·ªói:", err)
    return
end

fn()
